{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "no-submodule-imports": [
      true,
      "apollo-client"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "quotemark": [
      true,
      "double"
    ],
    "trailing-comma": {
      "options": {
        "multiline": "always",
        "singleline": "never",
        "esSpecCompliant": true
      }
    },
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-string-literal": false
  }
}
